<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.jasgames.ui.EstudianteWindow">
  <grid id="27dc6" binding="panelEstudianteMain" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="4ea7b" binding="panelHeaderEstudiante" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="34c61" class="javax.swing.JButton" binding="btnBackEstudiante">
            <constraints border-constraint="West"/>
            <properties>
              <text value="← Volver"/>
            </properties>
          </component>
          <component id="a2a96" class="javax.swing.JLabel" binding="lblTituloEstudiante">
            <constraints border-constraint="Center"/>
            <properties>
              <horizontalAlignment value="0"/>
              <text value="Modo Estudiante"/>
            </properties>
          </component>
          <grid id="8866f" binding="panelDatosEstudiante" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints border-constraint="East"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="4d758" class="javax.swing.JLabel" binding="lblNombreEstudiante">
                <constraints/>
                <properties>
                  <text value="Nombre:"/>
                </properties>
              </component>
              <component id="f544e" class="javax.swing.JTextField" binding="txtNombreEstudiante">
                <constraints/>
                <properties>
                  <columns value="15"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="e8a69" binding="panelSeleccionJuego" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="West"/>
        <properties>
          <minimumSize width="220" height="0"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="1435d" class="javax.swing.JLabel" binding="lblSeleccionJuego">
            <constraints border-constraint="North"/>
            <properties>
              <text value="Selecciona un juego:"/>
            </properties>
          </component>
          <scrollpane id="95f8c" binding="scrollJuegosEstudiante">
            <constraints border-constraint="Center"/>
            <properties>
              <minimumSize width="10" height="15"/>
            </properties>
            <border type="none"/>
            <children>
              <component id="1bebb" class="javax.swing.JList" binding="listaJuegosEstudiante">
                <constraints/>
                <properties/>
              </component>
            </children>
          </scrollpane>
        </children>
      </grid>
      <grid id="b7475" binding="panelJuegoEstudiante" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="9ed72" class="javax.swing.JLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Aquí se mostrará el juego seleccionado"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="8056a" layout-manager="GridLayoutManager" row-count="1" column-count="5" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="ee9ec" class="javax.swing.JLabel" binding="lblPuntajeActual">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Puntaje actual:"/>
            </properties>
          </component>
          <hspacer id="9d1cc">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="1" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
          </hspacer>
          <component id="d38f9" class="javax.swing.JLabel" binding="lblValorPuntaje">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="0"/>
            </properties>
          </component>
          <component id="75534" class="javax.swing.JButton" binding="btnFinalizarJuego">
            <constraints>
              <grid row="0" column="4" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Finalizar y guardar puntaje"/>
            </properties>
          </component>
          <component id="7291c" class="javax.swing.JButton" binding="btnIniciarJuego">
            <constraints>
              <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Iniciar juego"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
